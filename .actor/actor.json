{
	"actorSpecification": 1,
	"name": "export-dataset-to-s3",
	"title": "Export dataset from Actor to S3 bucket in Python",
	"description": "This actor will be use as integration to the scraping actor. It automate export dataset and gzip it, after that transfer the file to S3 bucket.",
	"version": "0.1",
	"storages": {
		"dataset": {
			"actorSpecification": 1,
			"views": {
				"overview": {
					"title": "Overview",
					"transformation": {
						"fields": [
							"start_at",
							"finish_at",
							"s3_path"
						]
					},
					"display": {
						"component": "table",
						"properties": {
							"start_at": {
								"label": "Start at",
								"format": "date"
							},
							"Finish_at": {
								"label": "Finish at",
								"format": "date"
							},
							"s3_path": {
								"label": "S3 Path",
								"format": "text"
							}
						}
					}
				}
			}
		}
	},
	"meta": {
		"templateId": "python-start"
	},
	"input": "./input_schema.json",
	"dockerfile": "./Dockerfile"
}
